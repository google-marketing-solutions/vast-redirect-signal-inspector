{
  "name": "vast-redirect-signal-inspector",
  "version": "2.2.0",
  "description": "The VAST Redirect Signal Inspector is an open-source tool designed to analyze and validate Google VAST redirects, ensuring optimal signal quality for IMA SDK, PAL SDK, and PAI implementations.",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:units && npm run test:e2e",
    "test:units": "karma start test/unit_tests/all.conf",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:javascript": "eslint .",
    "start": "webpack serve --config build/webpack.config.prod.babel.js --port 8085",
    "start:dev": "webpack serve --config build/webpack.config.dev.babel.js --port 8086",
    "start:lighthouse": "webpack serve --config build/webpack.config.lighthouse.babel.js --port 8086",
    "build": "webpack --config build/webpack.config.prod.babel.js",
    "build:deploy": "webpack --config build/webpack.config.deploy.babel.js",
    "build:dev": "webpack --config build/webpack.config.dev.babel.js",
    "upgrade": "npx npm-check -u --skip-unused && npm install && npm update && npm prune && npm audit fix && npx update-browserslist-db@latest",
    "postupgrade": "npm run test",
    "deploy": "gh-pages -d dist",
    "predeploy": "npm run test && npm run build:deploy",
    "lighthouse": "lhci autorun"
  },
  "author": "Markus Bordihn (mbordihn@google.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/node": "^7.28.0",
    "@babel/plugin-syntax-jsx": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/register": "^7.28.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@lhci/cli": "^0.15.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/joy": "^5.0.0-beta.51",
    "@mui/material": "^7.3.2",
    "@playwright/test": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint": "^9.35.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-jsdoc": "^55.0.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "gh-pages": "^6.1.1",
    "globals": "^16.3.0",
    "html-minimizer-webpack-plugin": "^5.0.3",
    "html-webpack-plugin": "^5.6.4",
    "jasmine-core": "^5.10.0",
    "json-minimizer-webpack-plugin": "^5.0.1",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^5.1.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-webpack": "^5.0.1",
    "lz-string": "^1.5.0",
    "mini-css-extract-plugin": "^2.9.4",
    "react-joyride": "^2.9.3",
    "style-loader": "^4.0.0",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
